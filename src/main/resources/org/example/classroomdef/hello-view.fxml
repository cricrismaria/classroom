<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.example.classroomdef.ui.ClassroomGUI">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="welcomeText" text="Welcome User!"/>
    <Label fx:id="secondWelcomeText" text="What action do you desire to do?"/>
    <Button fx:id="signUp" text="Sign Up" onAction="#onSignUpAction"/>
    <Button fx:id="login" text="Login" onAction="#onLoginAction"/>
</VBox>
